{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2022",
    "typeRoots": ["node_modules/@types"],
    "lib": ["ES2022", "dom", "dom.iterable"],
    "strict": true,
    "noEmit": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx"
  }
}
